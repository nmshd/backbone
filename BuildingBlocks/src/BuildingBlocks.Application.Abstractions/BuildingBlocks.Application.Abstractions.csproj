		<PackageReference Include="OpenIddict.EntityFrameworkCore" Version="5.2.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\DevelopmentKit.Identity\DevelopmentKit.Identity.csproj" />
		<ProjectReference Include="..\Tooling\Tooling.csproj" />
	</ItemGroup>

