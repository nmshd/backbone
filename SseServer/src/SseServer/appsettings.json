{
    "AllowedHosts": "*",
    "Modules": {
        "Devices": {
            "Application": {
                "AddressPrefix": "id1",
                "Pagination": {
                    "DefaultPageSize": 50,
                    "MaxPageSize": 200
                }
            }
        }
    },
    "Logging": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Backbone": "Information",

                "Microsoft": "Information",
                "Microsoft.AspNetCore": "Warning"
            }
        },
        "WriteTo": {
            "Console": {
                "Name": "Console",
                "Args": {
                    "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                }
            }
        }
    }
}
